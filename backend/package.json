{
  "name": "adelagio-backend",
  "version": "0.1.0",
  "description": "Adelagio Server",
  "keywords": [],
  "author": "Taras Halynskui",
  "engines": {
    "npm": "6.7.0",
    "node": "10.10.0"
  },
  "scripts": {
    "start": "node -optimize_for_size --max_old_space_size=460 --gc_interval=100 build/src/App.js",
    "dev:js": "node --require dotenv/config build/src/App.js",
    "dev:ts": "ts-node --require dotenv/config typescript/src/App.ts",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./build",
    "postinstall": "npm run build",
    "migrate:local": "flyway -configFiles=database/flyway-local.conf migrate",
    "migrate:development": "flyway -configFiles=database/flyway-development.conf migrate",
    "migrate:staging": "flyway -configFiles=database/flyway-staging.conf migrate",
    "migrate:production": "flyway -configFiles=database/flyway-production.conf migrate",
    "test:js": "mocha build/test/**/*.spec.js",
    "test:ts": "ts-mocha typescript/test typescript/test/**/*.spec.ts",
    "test-main:ts": "ts-node typescript/test/test.ts"
  },
  "dependencies": {
    "array-flatten": "^2.1.2",
    "array-sort": "^1.0.0",
    "await-timeout": "^0.3.0",
    "bcrypt": "3.0.0",
    "body-parser": "1.18.3",
    "bufferutil": "4.0.0",
    "change-case": "^3.1.0",
    "clone": "^2.1.2",
    "connect-pg-simple": "^5.0.0",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "express-ws": "4.0.0",
    "gamecenter-identity-verifier": "0.1.1",
    "google-auth-library": "^2.0.0",
    "guid-typescript": "^1.0.7",
    "http-status-codes": "1.3.0",
    "humps": "^2.0.1",
    "inversify": "4.13.0",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.1.0",
    "inversify-inject-decorators": "^3.1.0",
    "inversify-logger-middleware": "3.1.0",
    "lodash": "^4.17.11",
    "log4js": "3.0.6",
    "log4js-extend": "^0.2.1",
    "moment": "^2.22.2",
    "morgan": "1.9.1",
    "node-schedule": "^1.3.0",
    "passport": "0.4.0",
    "passport-custom": "1.0.5",
    "passport-local": "1.0.0",
    "pg-promise": "8.5.0",
    "reflect-metadata": "0.1.12",
    "rimraf": "^2.6.2",
    "shuffle-array": "^1.0.1",
    "superagent": "^4.0.0-beta.5",
    "swagger-express-ts": "1.0.1",
    "swagger-ui-dist": "3.19.3",
    "utf-8-validate": "5.0.1",
    "ws": "6.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/chai-http": "3.0.5",
    "@types/chai-things": "0.0.33",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/expect": "1.20.3",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.11",
    "@types/express-ws": "3.0.0",
    "@types/mocha": "5.2.5",
    "@types/node": "10.11.7",
    "@types/node-schedule": "^1.2.2",
    "@types/passport": "0.4.6",
    "@types/passport-local": "1.0.33",
    "@types/superagent": "^3.8.3",
    "@types/ws": "6.0.1",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "mocha": "5.2.0",
    "ts-mocha": "2.0.0",
    "ts-node": "7.0.1",
    "ts-node-dev": "1.0.0-pre.30",
    "typescript": "3.1.2",
    "flywaydb-cli": "git+https://github.com/Halynsky/flywaydb-cli.git#windows_fix"
  }
}
